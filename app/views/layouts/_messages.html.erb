<% flash.each do |name, msg| %>
  <div class="alert alert-<%= name %> alert-div alert-dismissible fade show" role="alert">
    <p><%= msg %></p>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
<% end %>

